    "@types/commander": "2.12.2",
    "@types/mocha": "10.0.1",
    "@types/node": "20.2.5",
    "@types/node-media-server": "^2.3.4",
    "chai": "^4.3.7",
    "mocha": "10.2.0",
    "ts-node": "10.9.1",
    "tslint": "6.1.3",
    "typescript": "5.1.3"
  }
